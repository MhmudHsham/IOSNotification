{
    "name": "mhmudhsham/iosnotification",
    "description": ":IOS Notification package",
    "license": "license",
    "authors": [
        {
            "name": "Mahmoud Gabr",
            "email": "mhmudhsham8@gmail.com",
            "homepage": "http://www.facebook.com/mhmudhsham"
        }
    ],
    "homepage": "https://github.com/mhmudhsham/iosnotification",
    "keywords": ["Laravel", "IOSNotification"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "mhmudhsham\\IOSNotification\\": "src/"
        },
        "files" : [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "mhmudhsham\\IOSNotification\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "mhmudhsham\\IOSNotification\\IOSNotificationServiceProvider"
            ],
            "aliases": {
                "IOSNotification": "mhmudhsham\\IOSNotification\\Facades\\IOSNotification"
            }
        }
    }
}
